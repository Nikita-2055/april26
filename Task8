#include<stdio.h>

int main(){
    int arr1[] = {1,2,3};
    int arr2[] = {4,5,6};
    int j = 3;
    int b = 2;
    int a = 2;
    for(int i = 0; i < 6; ++i){
        if(i < 3){
            printf("%d",arr1[a]);
            a--;
        }
        if(j == i && j < 6){
            printf("%d",arr2[b]);
            b--;
            j++;    
        }
    }
}
