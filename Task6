#include<stdio.h>

int main(){
    int size = 3;
    int arr1[size];
    int arr2[size];
    printf("input number in arr1:\n");
    for(int i = 0; i < size; ++i){
        scanf("%d",&arr1[i]);
    }
    printf("input numer in arr2:\n");
    for(int i = 0; i < size; ++i){
        scanf("%d",&arr2[i]);
    }
    for(int i = 0; i < size; ++i){
        if(arr1[i] != arr2[i]){
            printf("False:\n");
            return 0;
        }
    }
    printf("True:\n");
    return 0;
}
